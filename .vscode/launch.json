  "version": "0.2.0",
  "configurations": [
    {
      "name": "EQ (Windows)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/_deps/juce-build/extras/AudioPluginHost/AudioPluginHost_artefacts/Debug/AudioPluginHost.exe",
      "args": ["${workspaceRoot}/EQ/EQ.filtergraph"],
      "stopAtEntry": false,
      "console": "newExternalWindow",
      "preLaunchTask": "CMake: build"
    },
    {
      "name": "EQ (Linux)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/_deps/juce-build/extras/AudioPluginHost/AudioPluginHost_artefacts/Debug/AudioPluginHost",
      "args": ["${workspaceRoot}/EQ/EQ.filtergraph"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "CMake: build"
    },
    {
      "name": "AudioPluginHost Mac",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/_deps/juce-build/extras/AudioPluginHost/AudioPluginHost_artefacts/Debug/AudioPluginHost.app/Contents/MacOS/AudioPluginHost",
      "args": ["${workspaceFolder}/EQ"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "CMake: build",
      "MIMode": "lldb"
    },
    {
      "name": "DemoRunner (Windows)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/external/juce/DemoRunner.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "console": "newExternalWindow"
    }
  ]
}
